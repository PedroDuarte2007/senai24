#include <stdio.h>

int main() {
    
    int a, b, c;
    
    printf("Digite o numero de horas trabalhadas em um mÃªs:");
    scanf("%d", &a);
    
    printf("Digite o quanto voce ganha por hora trabalhada:R$");
    scanf("%d", &b);
    c = (a * b);
    
    printf("R$%d", c);
    
    return 0;

}
